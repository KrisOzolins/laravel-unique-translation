{
    "name": "krisozolins/laravel-unique-translation",
    "description": "Fork of codezero-be/laravel-unique-translation package",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Krisjanis Ozolins",
            "email": "kris.ozolins@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "orchestra/testbench": "~3.5.0",
        "phpunit/phpunit": "~6.0",
        "spatie/laravel-translatable": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "KrisOzolins\\UniqueTranslation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KrisOzolins\\UniqueTranslation\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KrisOzolins\\UniqueTranslation\\UniqueTranslationServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "sort-packages": true,
    "optimize-autoloader": true
}
